#include <stdio.h>

int main() {
    int n, i;
    float sum = 0.0;

    // Read number of elements
    scanf("%d", &n);

    int arr[n];

    // Read array elements
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    // Calculate mean
    float mean = sum / n;

    // Print mean value with 2 decimal places
    printf("Array Mean Value is %.2f\n", mean);

    return 0;
}
